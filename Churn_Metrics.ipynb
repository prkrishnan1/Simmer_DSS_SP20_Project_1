    "# Add Average Session Time Metric #"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 114,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Metric: avg_session_duration\n",
    "# Similar to \"average_session_time\", but NOT calculated the same way.\n",
    "# Takes the average of all of each user's session durations.\n",
    "\n",
    "def avg_session_time(people, events):\n",
    "    events = events[events['ae_session_length'] != -1.0]\n",
    "    groupby_id = events.groupby(['distinct_id']).agg(np.mean)\n",
    "    people_m2 = people.merge(groupby_id['ae_session_length'], on = 'distinct_id', how = 'left')\n",
    "    people_m2['ae_session_length'] = people_m2['ae_session_length'].fillna(-1)\n",
    "    people_m2 = people_m2.rename(columns={\"ae_session_length\": \"avg_session_duration\"})\n",
    "    return people_m2"
   ]
  },
