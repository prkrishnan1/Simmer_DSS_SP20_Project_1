   "source": [
    "# Add Slopes and Merge Slopes with People"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "def best_fit_slope(xs,ys):\n",
    "    m = (((np.mean(xs)*np.mean(ys)) - np.mean(xs*ys)) /\n",
    "         ((np.mean(xs)*np.mean(xs)) - np.mean(xs*xs)))\n",
    "    return m"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "def add_time_delta_slope(df):\n",
    "    df = df.loc[(events['name_x'] == '$ae_session') & (events['time_delta'] != 0)]\n",
    "    grouped = df.groupby('distinct_id', as_index=False).agg({'time' : 'count'})\n",
    "    grouped['time_delta_slope'] = None\n",
    "    for index, row in grouped.iterrows():\n",
    "        subset = df.loc[df['distinct_id'] == row['distinct_id']]\n",
    "        subset = subset.sort_values('time', ascending=True)\n",
    "        length = np.arange(len(subset))\n",
    "        slope = best_fit_slope(length, subset['time_delta'])\n",
    "        grouped.loc[grouped['distinct_id'] == row['distinct_id'], 'time_delta_slope'] = slope\n",
    "    final = grouped.drop(columns='time', axis=1)\n",
    "    return final"
   ]
  },
