    "events['time'] = events['time'].apply(lambda x : pd.to_datetime(x))\n",
    "most_recent = max(events['time'])\n",
    "threshold = most_recent - timedelta(days=90)\n",
    "threshold\n",
    "within90 = events.loc[events['time'] > threshold]"
   ]
